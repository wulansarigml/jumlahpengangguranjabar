--Membuat Database
CREATE DATABASE DISNAKER

--Membuat Table
CREATE TABLE JUMLAH_PENGANGGURAN_JABAR(
id BIGINT NOT NULL,
kode_provinsi BIGINT NOT NULL,
nama_provinsi VARCHAR(50) NOT NULL,
kode_kabupaten_kota BIGINT NOT NULL,
nama_kabupaten_kota VARCHAR(50) NOT NULL,
pendidikan VARCHAR(50) NOT NULL,
jumlah_pengangguran BIGINT,
satuan VARCHAR(10) NOT NULL,
tahun BIGINT
)
